{
	"_version": "1.8.0",
	"sap.app": {
		"id": "com.incture.lch.yardmanagement",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.38.11"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.incture.lch.yardmanagement.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.incture.lch.yardmanagement.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			],
			"js": [
				{
					"uri": "utility/GeoMap.js"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.incture.lch.yardmanagement.view",
				"controlAggregation": "pages",
				"controlId": "idAppYardMgmt",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "RouteApp",
					"pattern": "RouteApp",
					"target": [
						"TrailerList"
					]
				},
				{
					"name": "TrailerList",
					"pattern": "",
					"target": [
						"TrailerList"
					]
				},
				{
					"name": "YardHistory",
					"pattern": "YardHistory",
					"target": [
						"YardHistory"
					]
				},
				{
					"name": "GeoMap",
					"pattern": "GeoMap",
					"target": [
						"GeoMap"
					]
				}
			],
			"targets": {
				"TargetApp": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "App",
					"viewName": "App"
				},
				"TrailerList": {
					"viewType": "XML",
					"viewName": "TrailerList"
				},
				"YardHistory": {
					"viewType": "XML",
					"viewName": "YardHistory"
				},
				"GeoMap": {
					"viewType": "XML",
					"viewName": "GeoMap"
				}
			}
		}
	},
	"sap.platform.cf": {
		"oAuthScopes": [
			"$XSAPPNAME.display"
		]
	}
}