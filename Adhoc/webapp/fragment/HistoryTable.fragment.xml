		<ui:extension>
			<Toolbar class="sapMtoolBar">
				<content>
					<ToolbarSpacer/>
					<Button icon="sap-icon://customize" press="onCustomization"/>
				</content>
			</Toolbar>
		</ui:extension>
	</ui:Table>
