{
	"authenticationMethod": "route",
	"routes": [{
		"source": "^/lch_services/",
		"target": "/",
		"destination": "lch_services",
		"authenticationType": "xsuaa",
		"csrfProtection": false
	},{
		"source": "^/lch_service_wf/",
		"target": "/",
		"destination": "lch_service_wf",
		"authenticationType": "xsuaa",
		"csrfProtection": false
	}, {
		"source": "^/(.*)$",
		"target": "$1",
		"destination": "UserNode_api"
	}],
	"welcomeFile": "/cp.portal"
}