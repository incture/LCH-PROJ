{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.incture.lch.Adhoc",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		},
		"crossNavigation": {
			"inbounds": {
				"intent1": {
					"signature": {
						"parameters": {},
						"additionalParameters": "allowed"
					},
					"semanticObject": "",
					"action": ""
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.incture.lch.Adhoc.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.incture.lch.Adhoc.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.incture.lch.Adhoc.view",
				"controlAggregation": "pages",
				"controlId": "idLch_Adhoc_ToolPageContainer",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "App",
				"pattern": "App",
				"target": ["Order"]
			}, {
				"name": "History",
				"pattern": "history",
				"target": ["History"]
			}, {
				"name": "Order",
				"pattern": "Order/{accessMode}",
				"target": ["Order"]
			}, {
				"name": "Approval",
				"pattern": "Approval",
				"target": ["Approval"]
			}],
			"targets": {
				"App": {
					"viewName": "App"
				},
				"History": {
					"viewType": "XML",
					"viewName": "History"
				},
				"Order": {
					"viewType": "XML",
					"viewName": "Order"
				},
				"Approval": {
					"viewType": "XML",
					"viewName": "Approval"
				}
			}
		}
	}
}